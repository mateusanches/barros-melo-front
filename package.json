{
  "name": "barros-melo-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.23.0",
    "dateformat": "^5.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-currency-format": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-number-format": "^4.8.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "stylus": "^0.54.8",
    "sweetalert": "^2.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "build-css": "stylus -c src/client-side/static/stylus/ --out src/client-side/static/css",
    "watch-css": "yarn run build-css && stylus -c -w src/client-side/static/stylus --out src/client-side/static/css",
    "build-admin-css": "stylus -c src/admin-side/static/stylus/ --out src/admin-side/static/css",
    "watch-admin-css": "yarn run build-css && stylus -c -w src/admin-side/static/stylus --out src/admin-side/static/css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
